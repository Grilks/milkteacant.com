{
  "name": "milkteacant.com",
  "version": "1.0.0",
  "description": "Website for Alan Forsyth's book Milk Tea Can't",
  "main": "index.js",
  "author": "Callum Silcock <github@csi.lk> (https://csi.lk)",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm-run-all clean pages -p watch:pages watch:11ty",
    "clean": "rimraf dist src/_js",
    "pages": "webpack",
    "build": "npm-run-all clean build:pages build:11ty build:admin",
    "build:pages": "webpack --mode=production",
    "build:11ty": "eleventy --quiet",
    "build:admin": "cp src/admin/config.yml dist/admin/config.yml",
    "watch:pages": "webpack --watch",
    "watch:11ty": "eleventy --serve --quiet --watch",
    "lint": "eslint ./src/**/*.ts*",
    "test": "jest"
  },
  "dependencies": {
    "preact": "10.16.0",
    "preact-render-to-string": "6.2.0",
    "preactement": "1.8.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "@types/node": "14.18.54",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.45.0",
    "eslint-config-preact": "1.3.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "file-loader": "6.2.0",
    "glob": "10.3.3",
    "jest": "^29.6.2",
    "markdown-it": "13.0.1",
    "markdown-it-anchor": "8.6.7",
    "mini-css-extract-plugin": "2.7.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "sass": "1.64.1",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.4.4",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-assets-manifest": "5.1.0",
    "webpack-cli": "5.1.4",
    "yargs": "17.7.2"
  },
  "packageManager": "yarn@3.2.2"
}
